        android:text="Brett's Birthday"
        tools:layout_editor_absoluteX="98dp"
        tools:layout_editor_absoluteY="180dp" />

    <EditText
        android:id="@+id/editText5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
